<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.myfirstapp.searchnearestattractionapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.myfirstapp.searchnearestattractionapp.AppClass"
        android:allowBackup="true"
        android:icon="@drawable/suburban"
        android:label="@string/app_name"
        android:roundIcon="@drawable/suburban"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">

        <activity android:name="com.myfirstapp.searchnearestattractionapp.ui.base_search_act.AdvSearchAct">

        </activity>
        <activity android:name="com.myfirstapp.searchnearestattractionapp.ui.schedule_act.ScheduleActivity" />
        <activity android:name="com.myfirstapp.searchnearestattractionapp.ui.TestActivity">

        </activity>
        <activity android:name="com.myfirstapp.searchnearestattractionapp.MapAct" />
        <activity
            android:name="com.myfirstapp.searchnearestattractionapp.ui.add_points_act.AddPointsActivity"
            android:theme="@style/Theme.Transparent" />
        <activity android:name="com.myfirstapp.searchnearestattractionapp.ui.main_act.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>